{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "env": {
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "import/no-named-as-default": 0,
    "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "react/jsx-props-no-spreading": 0
  }
}
